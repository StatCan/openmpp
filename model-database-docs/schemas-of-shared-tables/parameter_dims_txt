CREATE TABLE parameter_dims_txt ( parameter_hid INT             NOT NULL,  dim_id        INT             NOT NULL,  lang_id       INT             NOT NULL,  descr         VARCHAR(255)    NOT NULL,  note          VARCHAR(32000),            PRIMARY KEY (parameter_hid, dim_id, lang_id), CONSTRAINT parameter_dims_txt_mk FOREIGN KEY (parameter_hid, dim_id) REFERENCES parameter_dims (parameter_hid, dim_id), CONSTRAINT parameter_dims_txt_lang_fk FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
