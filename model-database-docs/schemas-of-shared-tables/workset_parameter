CREATE TABLE workset_parameter ( set_id         INT NOT NULL,  parameter_hid  INT NOT NULL,  sub_count      INT NOT NULL,  default_sub_id INT NOT NULL,  PRIMARY KEY (set_id, parameter_hid), CONSTRAINT workset_parameter_mk FOREIGN KEY (set_id) REFERENCES workset_lst (set_id), CONSTRAINT workset_parameter_param_fk FOREIGN KEY (parameter_hid) REFERENCES parameter_dic (parameter_hid) )
