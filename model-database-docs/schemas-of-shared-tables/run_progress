CREATE TABLE run_progress ( run_id         INT         NOT NULL,  sub_id         INT         NOT NULL,  create_dt      VARCHAR(32) NOT NULL,  status         VARCHAR(1)  NOT NULL,  update_dt      VARCHAR(32) NOT NULL,  progress_count INT         NOT NULL,  progress_value FLOAT       NOT NULL,  PRIMARY KEY (run_id, sub_id), CONSTRAINT run_progress_mk FOREIGN KEY (run_id) REFERENCES run_lst (run_id) )
