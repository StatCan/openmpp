CREATE TABLE run_parameter_txt ( run_id        INT             NOT NULL,  parameter_hid INT             NOT NULL,  lang_id       INT             NOT NULL,  note          VARCHAR(32000),            PRIMARY KEY (run_id, parameter_hid, lang_id), CONSTRAINT run_parameter_txt_mk FOREIGN KEY (run_id) REFERENCES run_lst (run_id), CONSTRAINT run_parameter_txt_fk FOREIGN KEY (parameter_hid) REFERENCES parameter_dic (parameter_hid), CONSTRAINT run_parameter_txt_lang_fk FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
