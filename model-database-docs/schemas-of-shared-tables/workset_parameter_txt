CREATE TABLE workset_parameter_txt ( set_id        INT             NOT NULL,  parameter_hid INT             NOT NULL,  lang_id       INT             NOT NULL,  note          VARCHAR(32000),            PRIMARY KEY (set_id, parameter_hid, lang_id), CONSTRAINT workset_parameter_txt_mk FOREIGN KEY (set_id, parameter_hid) REFERENCES workset_parameter (set_id, parameter_hid), CONSTRAINT workset_parameter_txt_lang_fk FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
