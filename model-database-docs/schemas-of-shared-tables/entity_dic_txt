CREATE TABLE entity_dic_txt ( entity_hid INT            NOT NULL,  lang_id    INT            NOT NULL,  descr      VARCHAR(255)   NOT NULL,  note       VARCHAR(32000),           PRIMARY KEY (entity_hid, lang_id), CONSTRAINT entity_dic_txt_mk FOREIGN KEY (entity_hid) REFERENCES entity_dic (entity_hid), CONSTRAINT entity_dic_txt_lang_fk FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
