CREATE TABLE parameter_dic ( parameter_hid    INT          NOT NULL,  parameter_name   VARCHAR(255) NOT NULL,  parameter_digest VARCHAR(32)  NOT NULL,  parameter_rank   INT          NOT NULL,  type_hid         INT          NOT NULL,  is_extendable    SMALLINT     NOT NULL,  num_cumulated    INT          NOT NULL,  db_run_table     VARCHAR(64)  NOT NULL,  db_set_table     VARCHAR(64)  NOT NULL,  import_digest    VARCHAR(32)  NOT NULL,  PRIMARY KEY (parameter_hid), CONSTRAINT parameter_dic_un UNIQUE (parameter_digest), CONSTRAINT parameter_dic_type_fk FOREIGN KEY (type_hid) REFERENCES type_dic (type_hid) )
