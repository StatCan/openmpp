CREATE TABLE table_dims_txt ( table_hid INT             NOT NULL,  dim_id    INT             NOT NULL,  lang_id   INT             NOT NULL,  descr     VARCHAR(255)    NOT NULL,  note      VARCHAR(32000),            PRIMARY KEY (table_hid, dim_id, lang_id), CONSTRAINT table_dims_txt_mk FOREIGN KEY (table_hid, dim_id) REFERENCES table_dims (table_hid, dim_id), CONSTRAINT table_dims_txt_lang_fk FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
