CREATE TABLE table_dims ( table_hid   INT          NOT NULL,  dim_id      INT          NOT NULL,  dim_name    VARCHAR(255) NOT NULL,  type_hid    INT          NOT NULL,  is_total    SMALLINT     NOT NULL,  dim_size    INT          NOT NULL,  PRIMARY KEY (table_hid, dim_id), CONSTRAINT table_dims_mk FOREIGN KEY (table_hid) REFERENCES table_dic (table_hid), CONSTRAINT table_dims_type_fk FOREIGN KEY (type_hid) REFERENCES type_dic (type_hid) )
