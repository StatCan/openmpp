CREATE TABLE model_dic_txt ( model_id INT             NOT NULL,  lang_id  INT             NOT NULL,  descr    VARCHAR(255)    NOT NULL,  note     VARCHAR(32000),            PRIMARY KEY (model_id, lang_id), CONSTRAINT model_dic_txt_mk FOREIGN KEY (model_id) REFERENCES model_dic (model_id), CONSTRAINT model_dic_txt_lang_fk FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
