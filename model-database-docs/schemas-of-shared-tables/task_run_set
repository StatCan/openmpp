CREATE TABLE task_run_set ( task_run_id INT NOT NULL,  run_id      INT NOT NULL,  set_id      INT NOT NULL,  task_id     INT NOT NULL,  PRIMARY KEY (task_run_id, run_id), CONSTRAINT task_rs_un UNIQUE (task_run_id, set_id), CONSTRAINT task_rs_mk FOREIGN KEY (task_run_id) REFERENCES task_run_lst (task_run_id) )
