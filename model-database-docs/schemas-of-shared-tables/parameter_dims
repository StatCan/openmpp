CREATE TABLE parameter_dims ( parameter_hid INT          NOT NULL,  dim_id        INT          NOT NULL,  dim_name      VARCHAR(255) NOT NULL,  type_hid      INT          NOT NULL,  PRIMARY KEY (parameter_hid, dim_id), CONSTRAINT parameter_dims_mk FOREIGN KEY (parameter_hid) REFERENCES parameter_dic (parameter_hid), CONSTRAINT parameter_dims_type_fk FOREIGN KEY (type_hid) REFERENCES type_dic (type_hid) )
