CREATE TABLE type_dic_txt ( type_hid    INT             NOT NULL,  lang_id     INT             NOT NULL,  descr       VARCHAR(255)    NOT NULL,  note        VARCHAR(32000),            PRIMARY KEY (type_hid, lang_id), CONSTRAINT type_dic_txt_mk FOREIGN KEY (type_hid) REFERENCES type_dic (type_hid), CONSTRAINT type_dic_txt_lang_fk FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
