CREATE TABLE task_run_lst ( task_run_id INT          NOT NULL,  task_id     INT          NOT NULL,  run_name    VARCHAR(255) NOT NULL,  sub_count   INT          NOT NULL,  create_dt   VARCHAR(32)  NOT NULL,  status      VARCHAR(1)   NOT NULL,  update_dt   VARCHAR(32)  NOT NULL,  run_stamp   VARCHAR(32)  NOT NULL,  PRIMARY KEY (task_run_id), CONSTRAINT task_run_lst_mk FOREIGN KEY (task_id) REFERENCES task_lst (task_id) )
