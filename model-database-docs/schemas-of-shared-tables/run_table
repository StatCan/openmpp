CREATE TABLE run_table ( run_id       INT         NOT NULL,  table_hid    INT         NOT NULL,  base_run_id  INT         NOT NULL,  value_digest VARCHAR(32),           PRIMARY KEY (run_id, table_hid), CONSTRAINT run_table_mk FOREIGN KEY (run_id) REFERENCES run_lst (run_id), CONSTRAINT run_table_base_fk FOREIGN KEY (base_run_id) REFERENCES run_lst (run_id), CONSTRAINT run_table_fk FOREIGN KEY (table_hid) REFERENCES table_dic (table_hid) )
