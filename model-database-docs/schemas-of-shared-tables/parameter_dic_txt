CREATE TABLE parameter_dic_txt ( parameter_hid INT             NOT NULL,  lang_id       INT             NOT NULL,  descr         VARCHAR(255)    NOT NULL,  note          VARCHAR(32000),            PRIMARY KEY (parameter_hid, lang_id), CONSTRAINT parameter_dic_txt_mk FOREIGN KEY (parameter_hid) REFERENCES parameter_dic (parameter_hid), CONSTRAINT parameter_dic_txt_lang_fk FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
