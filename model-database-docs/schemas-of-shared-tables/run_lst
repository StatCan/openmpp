CREATE TABLE run_lst ( run_id        INT          NOT NULL,  model_id      INT          NOT NULL,  run_name      VARCHAR(255) NOT NULL,  sub_count     INT          NOT NULL,  sub_started   INT          NOT NULL,  sub_completed INT          NOT NULL,  sub_restart   INT          NOT NULL,  create_dt     VARCHAR(32)  NOT NULL,  status        VARCHAR(1)   NOT NULL,  update_dt     VARCHAR(32)  NOT NULL,  run_digest    VARCHAR(32)  NOT NULL,  value_digest  VARCHAR(32),            run_stamp     VARCHAR(32)  NOT NULL,  PRIMARY KEY (run_id), CONSTRAINT run_lst_mk FOREIGN KEY (model_id) REFERENCES model_dic (model_id) )
