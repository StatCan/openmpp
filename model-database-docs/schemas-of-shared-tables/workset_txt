CREATE TABLE workset_txt ( set_id   INT             NOT NULL,  lang_id  INT             NOT NULL,  descr    VARCHAR(255)    NOT NULL,  note     VARCHAR(32000),            PRIMARY KEY (set_id, lang_id), CONSTRAINT workset_txt_mk FOREIGN KEY (set_id) REFERENCES workset_lst (set_id), CONSTRAINT workset_txt_lang FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
