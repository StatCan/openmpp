CREATE TABLE model_table_dic ( model_id       INT      NOT NULL,  model_table_id INT      NOT NULL,  table_hid      INT      NOT NULL,  is_user        SMALLINT NOT NULL,  expr_dim_pos   INT      NOT NULL,  is_hidden      SMALLINT NOT NULL,  PRIMARY KEY (model_id, model_table_id), CONSTRAINT model_table_dic_un UNIQUE (model_id, table_hid), CONSTRAINT model_table_dic_mk FOREIGN KEY (table_hid) REFERENCES table_dic (table_hid), CONSTRAINT model_table_dic_fk FOREIGN KEY (model_id) REFERENCES model_dic (model_id) )
