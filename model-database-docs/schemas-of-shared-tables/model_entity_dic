CREATE TABLE model_entity_dic ( model_id        INT      NOT NULL,  model_entity_id INT      NOT NULL,  entity_hid      INT      NOT NULL,  PRIMARY KEY (model_id, model_entity_id), CONSTRAINT model_entity_dic_un UNIQUE (model_id, entity_hid), CONSTRAINT model_entity_dic_mk FOREIGN KEY (entity_hid) REFERENCES entity_dic (entity_hid), CONSTRAINT model_entity_dic_fk FOREIGN KEY (model_id) REFERENCES model_dic (model_id) )
