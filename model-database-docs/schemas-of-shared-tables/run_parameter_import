CREATE TABLE run_parameter_import ( run_id             INT          NOT NULL,  parameter_hid      INT          NOT NULL,  is_from_parameter  SMALLINT     NOT NULL,  from_model_id      INT          NOT NULL,  from_model_digest  VARCHAR(32)  NOT NULL,  from_run_id        INT          NOT NULL,  from_run_digest    VARCHAR(32)  NOT NULL,  PRIMARY KEY (run_id, parameter_hid), CONSTRAINT run_parameter_import_mk FOREIGN KEY (run_id, parameter_hid) REFERENCES run_parameter (run_id, parameter_hid) )
