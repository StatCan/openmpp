CREATE TABLE run_entity ( run_id         INT         NOT NULL,  entity_gen_hid INT         NOT NULL,  base_run_id    INT         NOT NULL,  row_count      INT         NOT NULL,  value_digest   VARCHAR(32),           PRIMARY KEY (run_id, entity_gen_hid), CONSTRAINT run_entity_mk FOREIGN KEY (run_id) REFERENCES run_lst (run_id), CONSTRAINT run_entity_base_fk FOREIGN KEY (base_run_id) REFERENCES run_lst (run_id), CONSTRAINT run_entity_gen_fk FOREIGN KEY (entity_gen_hid) REFERENCES entity_gen (entity_gen_hid) )
