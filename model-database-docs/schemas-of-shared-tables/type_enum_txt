CREATE TABLE type_enum_txt ( type_hid    INT             NOT NULL,  enum_id     INT             NOT NULL,  lang_id     INT             NOT NULL,  descr       VARCHAR(255)    NOT NULL,  note        VARCHAR(32000),            PRIMARY KEY (type_hid, enum_id, lang_id), CONSTRAINT type_enum_txt_mk FOREIGN KEY (type_hid) REFERENCES type_dic (type_hid), CONSTRAINT type_enum_txt_lang_fk FOREIGN KEY (lang_id) REFERENCES lang_lst (lang_id) )
